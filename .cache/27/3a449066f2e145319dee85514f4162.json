ine":176,"column":12},"generated":{"line":1,"column":3185}}],"sources":{"styles/styles.scss":":root {\n    --color-very-dark-blue: hsl(233, 47%, 7%);\n    --color-dark-desaturated-blue: hsl(244, 38%, 16%);\n    --color-soft-violet: hsl(277, 64%, 61%);\n\n    --color-white: hsl(0, 0%, 100%);\n    --color-slightly-transparent-white: hsla(0, 0%, 100%, 0.75);\n    --color-slightly-transparent-white-alt: hsla(0, 0%, 100%, 0.6);\n\n    --main-bg: var(--color-very-dark-blue);\n    --card-background: var(--color-dark-desaturated-blue);\n    --accent: var(--color-soft-violet);\n\n    --main-heading-color: var(--color-white);\n    --stats-color: var(--color-white);\n    --main-paragraph-color: var(--color-slightly-transparent-white);\n    --stat-heading-color: var(--color-slightly-transparent-white-alt);\n\n    --font-family-1: \"Inter\", sans-serif;\n    --font-family-2: \"Lexend Deca\", sans-serif;\n\n    --border-radius-size: 8px;\n}\n\nhtml {\n    font-size: 15px;\n    height: 100%;\n}\n\nbody {\n    height: 100%;\n    background: var(--main-bg);\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-family: var(--font-family-2);\n    color: var(--color-white);\n}\n\n.stat-card {\n    display: grid;\n    grid-template-columns: 1fr;\n    grid-template-rows: 240px 1fr;\n    grid-template-areas: \"image\" \"content\";\n    background: var(--card-background);\n    max-width: 1110px;\n    border-radius: var(--border-radius-size);\n    margin-left: 24px;\n    margin-right: 24px;\n\n    @media (min-width: 1440px) {\n        grid-template-columns: 1fr 1fr;\n        grid-template-rows: 1fr;\n        grid-template-areas: \"content image\";\n    }\n\n    &__wrapper {\n        flex: 1;\n        padding: 40px 32px;\n        grid-area: content;\n\n        @media (min-width: 1440px) {\n            padding: 70px 70px 60px 70px;\n        }\n    }\n\n    &__title {\n        margin-bottom: 30px;\n\n        h1 {\n            font-family: var(--font-family-1);\n            color: var(--main-heading-color);\n            font-size: 28px;\n            line-height: 32px;\n            font-weight: 700;\n            text-align: center;\n\n            .highlight {\n                color: var(--accent);\n            }\n\n            @media (min-width: 1440px) {\n                text-align: left;\n                font-size: 40px;\n                line-height: 48px;\n            }\n        }\n    }\n\n    &__content {\n        @media (min-width: 1440px) {\n            max-width: 90%;\n        }\n    }\n\n    &__description {\n        color: var(--main-paragraph-color);\n        font-family: var(--font-family-1);\n        font-weight: 400;\n        margin-bottom: 40px;\n        font-size: 15px;\n        line-height: 24px;\n        text-align: center;\n\n        @media (min-width: 1440px) {\n            margin-bottom: 80px;\n        }\n\n        @media (min-width: 1440px) {\n            text-align: left;\n        }\n    }\n\n    &__footer {\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        justify-content: center;\n\n        @media (min-width: 1440px) {\n            flex-direction: row;\n            justify-content: space-between;\n        }\n    }\n\n    &__stat {\n        text-align: center;\n        margin-bottom: 32px;\n\n        @media (min-width: 1440px) {\n            text-align: left;\n            margin-bottom: 0;\n        }\n    }\n\n    &__stat-title {\n        color: var(--stat-heading-color);\n        font-family: var(--font-family-1);\n        text-transform: uppercase;\n        font-size: 13px;\n        letter-spacing: 1px;\n    }\n\n    &__stat-content {\n        color: var(--stats-color);\n        font-family: var(--font-family-1);\n        font-weight: 700;\n        font-size: 28px;\n        margin-bottom: 8px;\n    }\n\n    &__image {\n        grid-area: image;\n        position: relative;\n        background: var(--accent);\n        background-image: url(\"../images/image-header-mobile.jpg\");\n        background-repeat: no-repeat;\n        background-blend-mode: overlay;\n        background-size: cover;\n\n        border-top-left-radius: var(--border-radius-size);\n        border-top-right-radius: var(--border-radius-size);\n\n        @media (min-width: 1440px) {\n            background-image: url(\"../images/image-header-desktop.jpg\");\n            border-top-left-radius: 0;\n            border-bottom-right-radius: var(--border-radius-size);\n        }\n\n        &::before {\n            content: \"\";\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 0;\n            bottom: 0;\n        }\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"9c0827a1c00775210fcfcc2126e63223","cacheData":{"env":{}}}